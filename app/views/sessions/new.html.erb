<h1>Log in</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email, :placeholder => "メールアドレス" %>
  <%= f.label :password %>
  <%= f.password_field :password, :placeholder => "パスワード" %>
  <%= f.submit "ログイン", class:"btn btn-info" %>
<% end %>
